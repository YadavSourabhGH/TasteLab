{
    "name": "tastelab",
    "version": "1.0.0",
    "description": "Real-time collaborative recipe editing platform",
    "scripts": {
        "dev": "concurrently \"npm run server\" \"npm run client\"",
        "server": "cd server && npm run dev",
        "client": "cd client && npm run dev",
        "install-all": "npm install && cd server && npm install && cd ../client && npm install"
    },
    "dependencies": {
        "concurrently": "^8.2.2"
    },
    "keywords": [
        "recipe",
        "collaboration",
        "real-time",
        "mern"
    ],
    "author": "",
    "license": "MIT"
}